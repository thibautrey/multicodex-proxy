{
  "name": "multicodex-proxy",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build:api": "tsc -p tsconfig.json",
    "build:web": "npm --prefix web run build",
    "build": "npm run build:web && npm run build:api",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
